    {
        type            noSlip;
    }

    outlet_0
    {
        type            zeroGradient;
    }

    outlet_1
    {
        type            zeroGradient;
    }

    inlet_0
    {
        type            zeroGradient;
    }

    inlet_1
    {
        type            zeroGradient;
    }

    inlet_2
    {
        type            zeroGradient;
    }

    inlet_3
    {
        type            zeroGradient;
    }

    inlet_4
    {
        type            zeroGradient;
    }

    inlet_5
    {
        type            zeroGradient;
    }

    inlet_6
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}

